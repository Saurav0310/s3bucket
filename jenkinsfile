pipeline{
    agent any
    
    stages{
        stage('check dir abd make dir'){
            steps{
                sh 'ls -ls'
                sh 'mkdir -p copy'
            
            }
        
        }
        stage('deploy to S3'){
            steps{
                sh 'aws s3 cp . s3://bucketttn1231 --recursive'
                sh 'aws s3 cp s3://bucketttn1231/b1.sh copy/.'
            } 
            
        }
        stage("read file"){
            steps{
                sh 'cat b1.sh'
            }
        
        }
        stage("delete file"){
            steps{
                sh 'rm b1.sh'
            }
        
        }
    
    }
}
